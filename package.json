        "postcss": "8.4.20",
        "postcss-less": "6.0.0",
        "prettier": "2.8.1",
        "react-redux": "8.0.5",
        "react-refresh": "0.14.0",
        "style-loader": "3.3.1",
        "stylelint": "14.16.0",
        "stylelint-config-prettier": "9.0.4",
        "stylelint-config-recess-order": "3.0.0",
        "stylelint-config-standard": "29.0.0",
        "stylelint-declaration-block-no-ignored-properties": "2.6.0",
