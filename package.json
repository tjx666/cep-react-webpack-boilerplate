{
    "name": "cep-react-webpack-boilerplate",
    "version": "0.0.1",
    "private": true,
    "author": "YuTengjing",
    "license": "MIT",
    "description": "An awesome cep extension boilerplate with excellent development experience",
    "scripts": {
        "start": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts/start.ts",
        "build": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts",
        "build:analyze": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --analyze",
        "bundle": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --all",
        "bundle:test": "cross-env NODE_ENV=test ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --all",
        "type-check": "tsc -p ./web/tsconfig.json --noEmit",
        "lint": "eslint -c ./JSX/.eslintrc.js JSX/**/*.jsx && eslint -c ./web/.eslintrc.js web/**/*.{js,ts,tsx}",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "prepare": "husky install"
    },
    "dependencies": {
        "abortcontroller-polyfill": "1.7.3",
        "axios": "0.27.2",
        "classnames": "2.3.1",
        "p-limit": "4.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.3.0",
        "react-use": "17.4.0"
    },
    "devDependencies": {
        "@babel/core": "7.18.13",
        "@babel/eslint-parser": "7.18.9",
        "@babel/plugin-proposal-decorators": "7.18.10",
        "@babel/plugin-transform-runtime": "7.18.10",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@babel/runtime": "7.18.9",
        "@gdcli/cli": "2.3.21",
        "@nuxt/friendly-errors-webpack-plugin": "2.5.2",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
        "@prettier/plugin-xml": "2.2.0",
        "@reduxjs/toolkit": "1.8.5",
        "@types/babel__core": "7.1.19",
        "@types/dateformat": "5.0.0",
        "@types/eslint": "8.4.6",
        "@types/glob": "7.2.0",
        "@types/gulp": "4.0.9",
        "@types/node": "16.11.56",
        "@types/react": "18.0.17",
        "@types/react-dom": "18.0.6",
        "@types/webpack-bundle-analyzer": "4.4.2",
        "@types/webpack-env": "1.18.0",
        "@types/yargs": "17.0.11",
        "@typescript-eslint/eslint-plugin": "5.35.1",
        "@typescript-eslint/parser": "5.35.1",
        "ansi-colors": "4.1.3",
        "babel-loader": "8.2.5",
        "clean-webpack-plugin": "4.0.0",
        "consola": "2.15.3",
        "core-js": "3.25.0",
        "cross-env": "7.0.3",
        "css-loader": "6.7.1",
        "dateformat": "5.0.3",
        "descriptor-info-type": "0.0.7",
        "eslint": "8.22.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-react": "7.31.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "execa": "5.1.1",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "7.2.13",
        "format-imports": "3.0.12",
