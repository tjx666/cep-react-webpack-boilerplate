{
    "name": "cep-react-webpack-boilerplate",
    "version": "0.0.1",
    "private": true,
    "author": "YuTengjing",
    "license": "MIT",
    "description": "An awesome cep extension boilerplate with excellent development experience",
    "scripts": {
        "start": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts/start.ts",
        "build": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts",
        "build:analyze": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --analyze",
        "bundle": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --all",
        "bundle:test": "cross-env NODE_ENV=test ts-node --files -P ./scripts/tsconfig.json ./scripts/build.ts --all",
        "type-check": "tsc -p ./web/tsconfig.json --noEmit",
        "lint": "eslint -c ./JSX/.eslintrc.js JSX/**/*.jsx && eslint -c ./web/.eslintrc.js web/**/*.{js,ts,tsx}",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "prepare": "husky install"
    },
    "dependencies": {
        "abortcontroller-polyfill": "1.7.5",
        "axios": "1.2.1",
        "classnames": "2.3.2",
        "p-limit": "4.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.4.5",
        "react-use": "17.4.0"
    },
    "devDependencies": {
        "@babel/core": "7.20.5",
        "@babel/eslint-parser": "7.19.1",
        "@babel/plugin-proposal-decorators": "7.20.5",
        "@babel/plugin-transform-runtime": "7.19.6",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@babel/runtime": "7.20.6",
        "@nuxt/friendly-errors-webpack-plugin": "2.5.2",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
        "@prettier/plugin-xml": "2.2.0",
        "@reduxjs/toolkit": "1.9.1",
        "@types/babel__core": "7.1.20",
        "@types/dateformat": "5.0.0",
        "@types/eslint": "8.4.10",
        "@types/glob": "8.0.0",
        "@types/gulp": "4.0.10",
        "@types/node": "18.11.15",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@types/webpack-bundle-analyzer": "4.6.0",
        "@types/webpack-env": "1.18.0",
        "@types/yargs": "17.0.17",
        "@typescript-eslint/eslint-plugin": "5.46.1",
        "@typescript-eslint/parser": "5.46.1",
        "ansi-colors": "4.1.3",
        "babel-loader": "9.1.0",
        "clean-webpack-plugin": "4.0.0",
        "consola": "2.15.3",
        "core-js": "3.26.1",
        "cross-env": "7.0.3",
        "css-loader": "6.7.3",
        "dateformat": "5.0.3",
        "descriptor-info-type": "0.0.7",
        "eslint": "8.29.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "execa": "5.1.1",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "7.2.14",
        "format-imports": "3.1.5",
        "glob": "8.0.3",
        "gulp": "4.0.2",
        "html-webpack-plugin": "5.5.0",
