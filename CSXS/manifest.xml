<?xml version="1.0" encoding="UTF-8" ?>
<ExtensionManifest
    Version="7.0"
    ExtensionBundleId="org.ytj.dev.boilerplate"
    ExtensionBundleVersion="0.0.1"
    ExtensionBundleName="boilerplateDev"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
    <ExtensionList>
        <Extension Id="org.ytj.dev.boilerplate.panel" Version="0.0.1" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PHSP" Version="20" />
            <Host Name="PHXS" Version="20" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="9" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="org.ytj.dev.boilerplate.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./web/public/redirect.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Boilerplate Dev</Menu>
                    <Geometry>
                        <Size>
                            <Height>611</Height>
                            <Width>280</Width>
                        </Size>
                        <MaxSize>
                            <Height>4000</Height>
                            <Width>600</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>611</Height>
                            <Width>280</Width>
                        </MinSize>
                    </Geometry>
                    <!-- <Icons>
                        <Icon Type="Normal">./CSXS/logo/light.png</Icon>
                        <Icon Type="RollOver">./CSXS/logo/light.png</Icon>
                        <Icon Type="DarkNormal">./CSXS/logo/light.png</Icon>
                        <Icon Type="DarkRollOver">./CSXS/logo/light.png</Icon>
                    </Icons> -->
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
